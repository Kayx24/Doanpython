<MainWidget>:
    MDNavigationLayout:
        ScreenManager:
            MDScreen:
                md_bg_color:"#efeff7"
                MDTopAppBar:
                    title:"Trang chủ"
                    anchor_title: 'left'
                    pos_hint: {"top": 1}
                    md_bg_color: "#3EAEF4"
                    specific_text_color: "#000000"
                    elevation: 5
                    left_action_items: [["menu", lambda x: root.open_nav_drawer()]]
                    #right_action_items: [["close", lambda x: root.close_nav_drawer()]]
                #Label:
                    #text: "Phần Mềm Nhận Diện Biển Báo Giao Thông"
                    #size_hint: 0.6, 0.5
                    #pos_hint: {'x': 0.20, 'y': 0.57}
                    #color: 1, 0, 0, 1  # Màu chữ: Đỏ (Red)
                    #font_size: "19sp"  # Kích thước phông chữ: 20sp
                MDIconButton:
                    id: magnifybutton
                    icon: 'magnify'
                    icon_color: 255,215,0
                    icon_size: "40dp"
                    size_hint: 0.15, 0.1
                    pos_hint: {'center_x':0.9, 'center_y': 0.85}
                MDBottomAppBar:
                    id: bottom_appbar
                    md_bg_color: "#F0E68C"
                    MDFabBottomAppBarButton:
                        id: fab_button
                        icon: "Images/scanIcon.jpg"
                        theme_bg_color: "Custom"
                        md_bg_color: "F0FFFF"
                        pos_hint: {'center_x': 0.5}  
                        on_press: root.btn_click_Quet()   
                GridLayout:
                    background_color:1, 0, 0, 1
                    pos_hint:{'x':0.075,'y':0.15}
                    size_hint:0.85,0.6
                    spacing:"10dp","10dp"
            
                    rows:4
                    cols:2
                    MDFloatingActionButton:
                        icon: "Images/uploadIcon.jpg"
                        icon_size:"110dp"
                        elevation_normal: 12
                        size_hint: 1,1
                        style: "tonal"
                        #user_font_size: "120sp"
                        md_bg_color: "#FFFAFA"
                        on_press: root.btn_click_UploadHinh() 
                    MDFloatingActionButton:
                        icon: "Images\SignInfoIcon.jpg"
                        icon_size:"110dp"
                        elevation_normal: 12
                        size_hint: 1,2
                        style: "tonal"
                        #user_font_size: "120sp"
                        md_bg_color: "#FFFAFA"
                        on_press: root.btn_click_TTBienBao()
                    MDLabel:
                        size_hint: 1, 1
                        pos_hint: {'center_x': 0.5, 'center_y': 0}
                        text:"    Upload hình ảnh"    
                    MDLabel:
                        size_hint: 1, 1
                        pos_hint: {'center_x': 0.5, 'center_y': 0}
                        text:"    Thông tin biển báo" 

                    MDFloatingActionButton:
                        icon: "Images/historyIcon.jpg"
                        icon_size:"130dp"
                        elevation_normal: 12
                        size_hint: 1,2
                        style: "tonal"
                        md_bg_color: "#FFFAFA"
                        on_press: root.btn_click_Tim()
                    MDFloatingActionButton:
                        icon: "Images/appInfoIcon.jpg"
                        icon_size:"110dp"
                        elevation_normal: 12
                        size_hint: 1,2
                        style: "tonal"
                        md_bg_color: "#FFFAFA"
                        on_press: root.btn_click_TTApp()
                    MDLabel:
                        size_hint: 1, 1
                        pos_hint: {'center_x': 0.5, 'center_y': 0}
                        text:"    Lịch Sử Tìm Kiếm"   
                    MDLabel:
                        size_hint: 1, 1
                        pos_hint: {'center_x': 0.5, 'center_y': 0}
                        text:"Thông Tin Ứng Dụng"

        MDNavigationDrawer:
            id: nav_drawer
            radius: [0, 16, 0, 16]
            md_bg_color:"#efeff7"
            MDNavigationDrawerMenu:
                auto_dismiss: False
                MDNavigationDrawerHeader:
                    id:hoten
                    source: "Images\__ruan_mei_honkai_and_1_more__fe90c2ad3c46efd002abe86229bcdc37.png"
                    title: "Username"
                    font_size: "15sp"
                    spacing: "10dp"
                    padding: [12, 0, 0, 50]

                MDNavigationDrawerLabel:
                    id:label_solanquettrongngay
                    text: "User"
                MDNavigationDrawerLabel:
                    id:label_tongsolanquet
                    text: "User"

                MDNavigationDrawerItem:
                    id:vip
                    icon: "star"
                    text_right_color: [0.29, 0.28, 0.22, 1]
                    text: "Purchase V.I.P"

                MDNavigationDrawerItem:
                    icon: "Images/shareIcon.png"
                    right_text: ""
                    text_right_color: [0.29, 0.28, 0.22, 1]
                    text: "Share"
                    spacing: "10dp"
                    padding: [10, 0, 0, 30]

                MDNavigationDrawerItem:
                    text: "               Log out"
                    on_press:root.close_nav_drawer()
                    on_press:root.manager.current='Main_login'
   
